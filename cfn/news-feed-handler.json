{
    "pipeline": {
        "name": "news-feed-handler",
        "roleArn": "arn:aws:iam::479756444842:role/service-role/codepipeline-nfh",
        "artifactStore": {
            "type": "S3",
            "location": "mgn-cicd-artifacts"
        },
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "name": "Source",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "ThirdParty",
                            "provider": "GitHub",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "Branch": "master",
                            "OAuthToken": "****",
                            "Owner": "andrewaddo",
                            "PollForSourceChanges": "false",
                            "Repo": "news-feed-handler"
                        },
                        "outputArtifacts": [
                            {
                                "name": "SourceArtifact"
                            }
                        ],
                        "inputArtifacts": [],
                        "region": "ap-southeast-1",
                        "namespace": "SourceVariables"
                    }
                ]
            },
            {
                "name": "Build",
                "actions": [
                    {
                        "name": "Build",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "provider": "CodeBuild",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ProjectName": "news-feed-handler-build"
                        },
                        "outputArtifacts": [
                            {
                                "name": "BuildArtifact"
                            }
                        ],
                        "inputArtifacts": [
                            {
                                "name": "SourceArtifact"
                            }
                        ],
                        "region": "ap-southeast-1",
                        "namespace": "BuildVariables"
                    }
                ]
            },
            {
                "name": "Deploy",
                "actions": [
                    {
                        "name": "Deploy",
                        "actionTypeId": {
                            "category": "Deploy",
                            "owner": "AWS",
                            "provider": "CloudFormation",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ActionMode": "CREATE_UPDATE",
                            "Capabilities": "CAPABILITY_IAM,CAPABILITY_AUTO_EXPAND",
                            "ParameterOverrides": "{\"Account\": \"479756444842\"}",
                            "RoleArn": "arn:aws:iam::479756444842:role/cfn-lambda-pipeline",
                            "StackName": "news-feed-handler",
                            "TemplatePath": "BuildArtifact::outputtemplate.yml"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": [
                            {
                                "name": "BuildArtifact"
                            }
                        ],
                        "region": "ap-southeast-1",
                        "namespace": "DeployVariables"
                    }
                ]
            },
            {
                "name": "DeployToTEST",
                "actions": [
                    {
                        "name": "Deploy",
                        "actionTypeId": {
                            "category": "Deploy",
                            "owner": "AWS",
                            "provider": "CloudFormation",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ActionMode": "CREATE_UPDATE",
                            "Capabilities": "CAPABILITY_IAM,CAPABILITY_AUTO_EXPAND",
                            "ParameterOverrides": "{\"Account\": \"323845505174\"}",
                            "RoleArn": "arn:aws:iam::323845505174:role/cfn-lambda-pipeline",
                            "StackName": "news-feed-handler-dev",
                            "TemplatePath": "BuildArtifact::outputtemplate.yml"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": [
                            {
                                "name": "BuildArtifact"
                            }
                        ],
                        "roleArn": "arn:aws:iam::323845505174:role/mgn-codepipeline-nfh",
                        "region": "ap-southeast-1"
                    }
                ]
            }
        ],
        "version": 7
    }
}
